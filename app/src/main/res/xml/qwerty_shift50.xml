<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="50dip"
    android:keyWidth="8%p"
    android:verticalGap="0px">

    <Row>
        <Key
            android:codes="96"
            android:isRepeatable="true"
            android:keyEdgeFlags="left"
            android:keyLabel="`"
            android:keyWidth="10%p" />

        <Key
            android:codes="33"
            android:isRepeatable="true"
            android:keyLabel="!" />
        <Key
            android:codes="34"
            android:isRepeatable="true"
            android:keyLabel="&quot;" />
        <Key
            android:codes="35"
            android:isRepeatable="true"
            android:keyLabel="#" />
        <Key
            android:codes="36"
            android:isRepeatable="true"
            android:keyLabel="$" />
        <Key
            android:codes="37"
            android:isRepeatable="true"
            android:keyLabel="%" />
        <Key
            android:codes="94"
            android:isRepeatable="true"
            android:keyLabel="^" />
        <Key
            android:codes="38"
            android:isRepeatable="true"
            android:keyLabel="&amp;" />
        <Key
            android:codes="42"
